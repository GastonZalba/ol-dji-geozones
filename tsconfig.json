{
  "compilerOptions": {
      "declaration": true,
      "allowJs": true,
      "downlevelIteration": true,
      "target": "ES2017",
      "jsx": "react",
      "jsxFactory": "myPragma",
      "jsxFragmentFactory": "null",
      "lib": ["ES2019", "dom"],
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "declarationDir": "dist",
      "skipLibCheck": true
  },
  "include": ["./src"]
}

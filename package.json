{
  "name": "ol-dji-geozones",
  "version": "2.0.2",
  "description": "Displays DJI Geo Zones on an OpenLayers map.",
  "browser": "dist/ol-dji-geozones.min.js",
  "main": "dist/ol-dji-geozones.js",
  "module": "lib/ol-dji-geozones.js",
  "types": "lib/ol-dji-geozones.d.ts",
  "style": "lib/ol-dji-geozones.css",
  "files": [
    "index.js",
    "lib/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "watch": "rollup -w --config rollup.config.dist.js --dev",
    "doc": "documentation readme src/ol-dji-geozones.ts --shallow --quiet=true --access=public --access=undefined --section=API --parse-extension ts && node scripts/external-links.js README.md && prettier --write README.md",
    "format": "eslint --fix src/*.ts && prettier --write src",
    "lint": "eslint src/*.ts && prettier --check src",
    "build": "npm run lint && rollup --config rollup.config.dist.js && rollup --config rollup.config.js"
  },
  "keywords": [
    "ol",
    "openlayers",
    "dji",
    "uav",
    "vant",
    "drone",
    "geozone",
    "geo zones",
    "flyzafe",
    "airports",
    "aerodrome",
    "airspace",
    "mavic",
    "phantom 4"
  ],
  "author": "GastÃ³n Zalba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GastonZalba/ol-dji-geozones.git"
  },
  "bugs": {
    "url": "https://github.com/GastonZalba/ol-dji-geozones/issues"
  },
  "homepage": "https://github.com/GastonZalba/ol-dji-geozones#readme",
  "dependencies": {},
  "peerDependencies": {
    "ol": ">=5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/node": "^14.18.32",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.12",
    "documentation": "^14.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "ol": "^7.1.0",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.55.0",
    "typescript": "^4.8.4"
  },
  "overrides": {
    "rollup-plugin-terser": {
      "terser": "^5.15.0"
    }
  }
}

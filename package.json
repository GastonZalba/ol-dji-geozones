{
  "name": "ol-dji-geozones",
  "version": "1.0.0",
  "description": "Display DJI Geo Zones in an OpenLayers map.",
  "main": "dist/ol-dji-geozones.js",
  "module": "dist/ol-dji-geozones.js",
  "types": "dist/ol-dji-geozones.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "nodemon --watch 'src/**' -e ts,css --ignore 'dist/' --exec npm run build",
    "precleanstart": "del-cli dist tmp",
    "doc": "documentation readme src/ol-dji-geozones.ts --quiet=true --access=public --access=undefined --section=API --parse-extension ts && node scripts/external-links.js README.md && prettier --write README.md",
    "format": "eslint --fix src/*.ts && prettier --write src",
    "lint": "eslint src/*.ts && prettier --check src",
    "copy-assets": "ncp src/assets/ tmp/assets/",
    "build": "npm run precleanstart && tsc --pretty && npm run copy-assets && rollup --config rollup.config.js",
    "postbuild": "ncp src/ol-dji-geozones.css dist/ol-dji-geozones.css && del-cli dist/assets tmp"
  },
  "keywords": [
    "ol",
    "openlayers",
    "open layers",
    "dji",
    "uav",
    "vant",
    "drone",
    "geozone",
    "geo zones",
    "flyzafe",
    "airports",
    "aerodrome",
    "airspace",
    "mavic",
    "phantom 4"
  ],
  "author": "GastÃ³n Zalba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GastonZalba/ol-dji-geozones.git"
  },
  "bugs": {
    "url": "https://github.com/GastonZalba/ol-dji-geozones/issues"
  },
  "homepage": "https://github.com/GastonZalba/ol-dji-geozones#readme",
  "dependencies": {},
  "peerDependencies": {
    "ol": ">=5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.1.0",
    "@types/node": "^14.14.10",
    "@types/ol": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "del-cli": "^3.0.1",
    "documentation": "^13.1.0",
    "eslint-config-openlayers": "^14.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.6",
    "ol": "^6.1.1",
    "prettier": "^2.2.0",
    "rollup": "^2.33.3",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "globals": {
      "ol": "readonly",
      "document": "writable"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "quoteProps": "consistent",
    "bracketSpacing": true
  }
}
